{
  "private": true,
  "scripts": {
    "dev": "run-p watch serve:dev",
    "serve:dev": "npm run preview -- --no-browser",
    "preview": "live-server public --host=localhost --port=3000",
    "compile": "babel src --out-dir public/scripts",
    "watch": "npm run compile -- --watch",
    "prod": "babel src --out-dir public/minified --minified",
    "bundle": "webpack --node-env development -c ./webpack.config.js",
    "build": "webpack build --node-env production -c ./webpack.config.js"
  },
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "live-server": "1.2.2",
    "npm-run-all": "4.1.5",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1"
  }
}