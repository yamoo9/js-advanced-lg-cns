    "watch": "npm run compile -- --watch",
    "build": "babel src --out-dir public/minified --minified"
  },
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "live-server": "1.2.2",
    "npm-run-all": "4.1.5"
  }
}
